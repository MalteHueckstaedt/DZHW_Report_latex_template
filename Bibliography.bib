@article{deutscheforschungsgemeinschaftInterdisziplinareZusammenarbeitExzellenzinitiative2015,
  title = {Interdisziplinäre {{Zusammenarbeit}} in Der {{Exzellenzinitiative}}},
  author = {{Deutsche Forschungsgemeinschaft}},
  date = {2015-10-01},
  journaltitle = {Förderatlas 2015},
  issn = {9783527800261},
  doi = {10.1002/9783527800261.ch5},
  url = {https://onlinelibrary.wiley.com/doi/10.1002/9783527800261.ch5},
  urldate = {2019-05-27},
  abstract = {Summary Dieses Kapitel beschreibt wie Mittel aus der Exzellenzinitiative genutzt werden, um die interdisziplinäre Zusammenarbeit zwischen Fächern zu unterstützen. Die Analysen gehen der Frage nach der Gestalt interdisziplinärer Forschung mit einfachen Mitteln und allein deskriptivem Anspruch nach. Zugrunde gelegt werden hierfür maßgeblich für Zwecke der Öffentlichkeitsarbeit erhobene Daten, die hier erstmals für statistische Analysen genutzt werden. Im Mittelpunkt der folgenden Betrachtung stehen die Förderlinien Graduiertenschulen und Exzellenzcluster der Exzellenzinitiative. Die Graduiertenschulen erweisen sich demgegenüber als überdurchschnittlich den Geistes- und Sozialwissenschaften zugewandt. Die eben vorgestellten Analysen haben einen ersten Eindruck des hohen Stellenwerts fachübergreifender Zusammenarbeit in den beiden hier betrachteten Förderlinien der Exzellenzinitiative vermittelt. Am häufigsten werden für diesen Wissenschaftsbereich die Fachrichtungen Geschichtswissenschaften und Soziologie genannt, und auch die Politikwissenschaft vereint eine größere Zahl Nennungen auf sich. Die hier abschließend vorgestellten Netzwerkanalysen bieten einen Eindruck von den Strukturwirkungen, die aus der gemeinsamen Beteiligung von Fachrichtungen an Graduiertenschulen und Exzellenzclustern resultieren.},
  file = {/Users/maltehueckstaedt/Google Drive/Zotero_File/Deutsche Forschungsgemeinschaft_2015_Interdisziplinäre.pdf},
  series = {Wiley {{Online Books}}}
}

