@article{deutscheforschungsgemeinschaftEinrichtungenUndRegionen2015,
  title = {Einrichtungen Und {{Regionen}} Der {{Forschung}} in {{Deutschland}}},
  author = {{Deutsche Forschungsgemeinschaft}},
  date = {2015-10-01},
  journaltitle = {Förderatlas 2015},
  issn = {9783527800261},
  doi = {10.1002/9783527800261.ch3},
  url = {https://onlinelibrary.wiley.com/doi/10.1002/9783527800261.ch3},
  urldate = {2019-05-27},
  abstract = {Summary Dieses Kapitel beschreibt die mittelempfangenden Einrichtungen und die von diesen geprägten Forschungsregionen der Forschung in Deutschland. Im Fokus des DFG-Förderatlas steht die öffentlich finanzierte Forschung an Hochschulen und außeruniversitären Forschungseinrichtungen. In diesem Kapitel erfolgt zunächst ein Überblick zu diesen Kennzahlen in der Differenzierung nach den Einrichtungsarten, an denen Wissenschaftlerinnen und Wissenschaftler mit den entsprechenden Mitteln Forschungsprojekte durchführen. Bezogen auf die außeruniversitäre Forschung erfolgt eine ergänzende Darstellung der Profildaten für die großen Wissenschaftsorganisationen. Deutschland weist eine Infrastruktur für Forschung und Entwicklung auf, die durch eine Vielzahl regionaler Verdichtungsräume geprägt ist. Forschung in Deutschland ist polyzentrisch, also nicht auf ein einzelnes Hauptzentrum fokussiert, sondern für viele Regionen strukturbildend. Der DFG-Förderatlas legt mit der hier vorgelegten Ausgabe einen thematischen Schwerpunkt auf die Exzellenzinitiative. Abschließend stellt das Kapitel Befunde einer bibliometrischen Analyse zu Unterschieden im Publikationsaufkommen von Hochschulen mit und ohne Beteiligung an den Förderlinien Graduiertenschulen und Exzellenzcluster vor.},
  file = {/Users/maltehueckstaedt/Google Drive/Zotero_File/Deutsche Forschungsgemeinschaft_2015_Einrichtungen und.pdf},
