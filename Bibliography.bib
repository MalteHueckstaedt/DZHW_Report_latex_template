@article{vanrijnsoeverResourcebasedViewInteractions2008,
  title = {A Resource-Based View on the Interactions of University Researchers},
  author = {van Rijnsoever, Frank J. and Hessels, Laurens K. and Vandeberg, Rens L. J.},
  date = {2008},
  journaltitle = {Research Policy},
