@inproceedings{humphreysMixingMethodsBayesian2014,
  title = {Mixing {{Methods}} : {{A Bayesian Integration}} of {{Qualitative}} and {{Quantitative Inferences Version}} 2 . 0},
  shorttitle = {Mixing {{Methods}}},
  author = {Humphreys, Macartan and Jacobs, Alan H.},
  date = {2014},
  abstract = {Some scholars have suggested that quantitative and qualitative approaches to causal inference can be unified under an essentially correlational framework. Other scholars have argued that such a conceptualization does not exploit the non-correlational information that is central to process tracing. We formalize an alternative unification —Bayesian Integration of Quantitative and Qualitative data (BIQQ) — that allows researchers to draw causal inferences from combinations of withinand cross-case observations given prior beliefs about causal e↵ects, assignment propensities, and the informativeness of di↵erent types of within-case observations. The framework further generates learning about the assumptions underlying correlational and process-based inference. We illustrate the approach with simulated data and with two applications to substantive issues that have received significant quantitative and qualitative treatment in political science: the origins of electoral systems and the causes of civil war. Finally, we demonstrate how the framework can yield guidance on research design, presenting results on the optimal combinations of within-case and cross-case data collection under di↵erent research conditions. ⇤Department of Political Science, Columbia University. Email: mh2245@columbia.edu Our thanks for comments on an earlier version of this paper to Andrew Bennett, Colin Elman, Andrew Gelman, Don Green, Elizabeth King, and participants at the IQMMR Authors’ Workshop at Syracuse University. Our thanks too to Andrew Guess and Sarah Khan for terrific research assistance and their many generous comments. †Department of Political Science, UBC. Email: alan.jacobs@ubc.ca},
  file = {/Users/maltehueckstaedt/Google Drive/Zotero_File/Humphreys&Jacobs_2014_Mixing Methods.pdf},
  keywords = {Approximation algorithm,Bayesian network,Causal filter,Causal inference,Causality,Complement (complexity),Conceptualization (information science),Diabetes Insipidus,Diazooxonorleucine,Ectomesenchymal Chondromyxoid Tumor,Email,Embedded system,Experiment,explanation,Information,King's Evil,Language primitive,Level of measurement,Maximal set,National origin,Ontology (information science),Population Parameter,Probability,Simulation,Theory}
}

