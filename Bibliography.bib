@article{deutscheforschungsgemeinschaftOffentlichGeforderteForschung2015,
  title = {Öffentlich Geförderte {{Forschung}} in {{Deutschland}} – Ein {{Überblick}}},
  author = {{Deutsche Forschungsgemeinschaft}},
  date = {2015-10-01},
  journaltitle = {Förderatlas 2015},
  issn = {9783527800261},
  doi = {10.1002/9783527800261.ch2},
  url = {https://onlinelibrary.wiley.com/doi/10.1002/9783527800261.ch2},
  urldate = {2019-05-27},
  abstract = {Summary Dieses Kapitel bietet zunächst eine international vergleichende Übersicht zu den personellen und finanziellen Ressourcen für Forschung und Entwicklung (FuE). Dabei werden insbesondere die unterschiedlichen Forschungsstrukturen der zum Vergleich herangezogenen Länder thematisiert. Anschließend erfolgt eine genauere Betrachtung der Struktur und Finanzierung der deutschen Forschungslandschaft. Wie bereits im Deutsche Forschungsgemeinschaft (DFG)-Förderatlas 2012 festgestellt wurde, nimmt der Anteil von Drittmitteln zur Finanzierung von Forschung an Hochschulen im Zeitverlauf kontinuierlich zu. Die Deutsche Forschungsgemeinschaft ist die zentrale Förderorganisation für die Forschung in Deutschland. Im Spezifischen Programm Zusammenarbeit werden (zumeist) großformatige grenzüberschreitende Verbundvorhaben zwischen Hochschulen, Industrie und Forschungseinrichtungen gefördert, wobei solche Verbünde Kooperationspartner aus normalerweise mindestens drei Ländern aufweisen. Von besonderer Bedeutung für die weitere Entwicklung des Europäischen Forschungsraums ist die Förderung des wissenschaftlichen Nachwuchses. Abschließend gibt das Kapitel einen kompakten Überblick zu den zentralen öffentlichen Fördermittelgebern in Deutschland, die den DFG-Förderatlas mit der Bereitstellung ihrer Förderdaten unterstützen.},
  file = {/Users/maltehueckstaedt/Google Drive/Zotero_File/Deutsche Forschungsgemeinschaft_2015_Öffentlich.pdf},
  series = {Wiley {{Online Books}}}
}

