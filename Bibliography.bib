  file = {/Users/maltehueckstaedt/Google Drive/Zotero_File/Stegmueller_2013_How Many Countries.pdf;/Users/maltehueckstaedt/Google Drive/Zotero_File/Stegmueller_2013_How Many Countries2.pdf;/Users/maltehueckstaedt/Zotero/storage/EDYI6J3R/Stegmueller2013How Many Countries for Multilevel Modeling - A Comparison of Frequentist and Bayesian Approaches.pdf;/Users/maltehueckstaedt/Zotero/storage/NNPCMB9J/Stegmueller2013How Many Countries for Multilevel Modeling - A Comparison of Frequentist and Bayesian Approaches.pdf},
